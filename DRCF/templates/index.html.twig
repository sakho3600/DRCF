    <script src="assets/js/print/jQuery.print.min.js"></script>
    <script src="assets/js/print/buttons.print.min.js"></script>
    <script src="assets/js/print/dataTables.buttons.min.js"></script>
    <script src="assets/js/print/buttons.flash.min.js"></script>
